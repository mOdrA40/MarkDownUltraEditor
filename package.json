{
  "name": "markdownultraremix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "lint:check": "biome lint .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check --write .",
    "ci": "biome ci",
    "prebuild": "bun run check",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "build:analyze": "ANALYZE=true react-router build",
    "preview": "react-router-serve ./build/server/index.js",
    "security:audit": "node scripts/security-audit.js",
    "security:audit:demo": "node scripts/security-audit.js --demo",
    "security:check": "bun run security:audit && bun run typecheck && bun run lint:check",
    "predeploy": "bun run security:check && bun run build",
    "bundle-size": "du -sh build/ && find build -name '*.js' -exec du -h {} + | sort -hr",
    "clean": "rm -rf build dist .react-router node_modules/.cache"
  },
